, params) {\\n    Object.assign(this, params)\\n    this.arr = []\\n    this.sign = '$' + (this.name || '') + '_'\\n    this.color = this.color || '#12b668'\\n    this.boundary = this.boundary || 'blank_block'\\n    this.push.apply(this, classArray)\\n}\\nClassTab.prototype = {\\n    constructor: ClassTab,\\n    load(el) {\\n        let folded = this.fold && getMyVar('$fold_' + this.sign, '')\\n        if (this.arr.length > 1 && this.fold) el.push({\\n            title: '““””' + (folded ? '▶' : '▼').fontcolor('#049eff'),\\n            url: $('#noLoading#').lazyRule((sign, folded) => {\\n                putMyVar('$fold_' + sign, folded ? '' : 'T')\\n                refreshPage(false)\\n                return 'hiker://empty'\\n            }, this.sign, folded),\\n            col_type: 'scroll_button'\\n        })\\n        let arr = folded ? [this.arr[0]] : this.arr\\n        arr.forEach((v) => {\\n            let { id, name, url } = v,\\n            selected = this.getClass(id)\\n            name.forEach((name, i) => {\\n                let now = { name: name, url: url[i] },\\n                    isSelected = this.classEqual(selected, now, id)\\n                el.push({\\n                    title: isSelected ? '““””' + name.bold().fontcolor(this.color) : name,\\n                    url: isSelected ? 'hiker://empty' : $('#noLoading#').lazyRule((sign, id, now) => {\\n                        putMyVar(sign + id, JSON.stringify(now))\\n                        putMyVar('$lastId_' + sign, id)\\n                        refreshPage(false)\\n                        return 'hiker://empty'\\n                    }, this.sign, id, now),\\n                    col_type: 'scroll_button'\\n                })\\n            })\\n            el.push({ col_type: this.boundary })\\n        })\\n    },\\n    init(classObj) {\\n        let [parseL, parseI, html] = classObj.parse || []\\n        if (parseL && parseI && html) {\\n            let { id, name, url, index } = classObj\\n        